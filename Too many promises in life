const user = {
    username: 'Kaushal',
    lastactivitytime: '08 August'
}

function updatelastactivitytime() {
    return new Promise((resolve, reject) = {
        setTimeout(() => {
            user.lastactivitytime = newDate().getTime();
            resolve(user.lastactivitytime)
        }, 1000)
    })
}

